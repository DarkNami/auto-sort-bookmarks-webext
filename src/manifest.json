{
  "id": "sortbookmarks@bouanto",
  "manifest_version": 2,
  "name": "Auto-Sort Bookmarks",
  "default_locale": "en_US",
  "version": "3.0",
  "version_name": "3.0 beta 2",
  "author": "Eric Bixby",
  "description": "Sort bookmarks by multiple criteria",
  "background": {
    "scripts": [
      "background/default-prefs.js",
      "background/main.js"
    ],
    "persistent": true
  },
  "icons": {
    "32": "content/images/icon-32.png",
    "40": "content/images/icon-40.png",
    "48": "content/images/icon-48.png",
    "128": "content/images/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "40": "content/images/icon-40.png"
    },
    "default_title": "Auto-Sort Bookmarks",
    "default_popup": "content/popup.html"
  },
  "options_page": "content/settings.html?panel=settings",
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}
